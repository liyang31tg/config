function proxy(){
  nohup shadowsocks2-macos -c 'ss://AEAD_CHACHA20_POLY1305:hZYOgw61z41mb4q1@[tg-hy-test]:8678' \
        -verbose -socks :1080 -u -udptun :8053=8.8.8.8:53,:8054=8.8.4.4:53 \
                                     -tcptun :8053=8.8.8.8:53,:8054=8.8.4.4:53 &
  # nohup ssh -Nfg -D 127.0.0.1:1080 root@tg-uf-test > proxy.log 2>&1 &
}

function unproxy(){
 if  pkill shadowsocks2-macos;then
   proxy_off
 fi
}

function proxy_off(){
  unset all_proxy
  echo "************ 关闭当前终端代理 **********"
}

function proxy_on(){
  export all_proxy=socks5://127.0.0.1:1080
  echo "************ 开启当前终端代理 **********"
 if ! curl cip.cc ;then
   echo "************开启socket5***************"
   proxy
   curl cip.cc
 fi
}
if [ -z $KITTY_PID ];then
proxy_on
fi




function fg(){
  if [[ $# -eq 1 && $1 = - ]];then
    builtin fg %-
  else
    builtin fg %"$@"
  fi
}
function ip(){
  curl cip.cc
  #curl http://pv.sohu.com/cityjson
}
